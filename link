<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Random Survey Redirect</title>
    <script>
        // List of survey URLs
        const surveyUrls = [
            "https://docs.google.com/forms/d/e/1FAIpQLScEZ493YxO17zT3mHpiNTmTe8cHnNSog909AbpTvqYmv_zoTA/viewform?usp=header",
            "https://docs.google.com/forms/d/e/1FAIpQLScveSnqQNAbDMVkOxUETU_GNA4LvYgNOj9_FTekMmaT8GqI6Q/viewform?usp=header",
            "https://docs.google.com/forms/d/e/1FAIpQLScoqq2HOI9PD766T_HLzrEXs7Nq0nL8UAA2Tq_KM18Lhs_jYA/viewform?usp=header",
            "https://docs.google.com/forms/d/e/1FAIpQLSfbff-NieUmRVzyPU_-6imUkakn2PnrlqsP2WLSqEcwXWTI_Q/viewform?usp=header",
            "https://docs.google.com/forms/d/e/1FAIpQLScTx2HPHdCrvUu7g6lmAV70yALEswDFtm1kkNab5-t4X2dI2g/viewform?usp=header",
            "https://docs.google.com/forms/d/e/1FAIpQLSeXMgE_N19Bh2iaB6IQ28s-DR996bYY-LHon1M20Ld7gFzwSA/viewform?usp=header"
        ];

        // Redirect to a random URL
        function redirectToRandomSurvey() {
            const randomIndex = Math.floor(Math.random() * surveyUrls.length);
            window.location.href = surveyUrls[randomIndex];
        }
    </script>
</head>
<body onload="redirectToRandomSurvey()">
    <h1>Redirecting to a random survey...</h1>
    <p>If you are not redirected automatically, <a href="#" onclick="redirectToRandomSurvey()">click here</a>.</p>
</body>
</html>
